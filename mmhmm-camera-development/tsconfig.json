{
  "compilerOptions": {
    "target": "Es2020",
    "module": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "strict": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "teleport/src/**/*",
  ]
}
