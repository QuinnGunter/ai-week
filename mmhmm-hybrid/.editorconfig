# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{h,cc,m,mm}]
indent_style = space
indent_size = 2

[mac/**.{h,cc,m,mm,swift}]
indent_style = tab
indent_size = 4
